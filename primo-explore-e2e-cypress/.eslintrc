{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "semi": [
      "error",
      "never",
      { "beforeStatementContinuationChars": "always" }
    ],
    "no-unused-vars": [
      "warn"
    ]
  },
  "globals": {
    "angular": false,
    "cy": true,
    "Cypress": true
  },
  "env": {
    "jest": true
  }
}